{
  "name": "@l1m/api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "private": true,
  "dependencies": {
    "@l1m/core": "^0.1.3",
    "@ts-rest/fastify": "^3.52.0",
    "@types/ioredis": "^4.28.10",
    "fastify": "^4.26.1",
    "ioredis": "^5.5.0",
    "tsx": "^4.19.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx -r dotenv/config --watch src/server.ts",
    "test": "jest --config=jest.config.js",
    "build": "tsc --build",
    "watch": "tsc -w",
    "format": "prettier --write \"**/*.{ts,js,json,md,html,css}\"",
    "docker:build": "docker build -t l1m .",
    "docker:publish": "docker tag l1m inferable/l1m:latest && docker push inferable/l1m:latest"
  }
}
