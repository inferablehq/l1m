{
  "name": "@l1m/api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@boundaryml/baml": "^0.76.2",
    "@ts-rest/fastify": "^3.52.0",
    "@types/ioredis": "^4.28.10",
    "@upstash/redis": "^1.34.4",
    "ajv": "^8.17.1",
    "dereference-json-schema": "^0.2.1",
    "fastify": "^4.26.1",
    "file-type": "^20.3.0",
    "ioredis": "^5.5.0",
    "jsonschema": "^1.5.0",
    "tsx": "^4.19.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.8",
    "@types/node": "^20.11.24",
    "dotenv": "^16.4.7",
    "prettier": "^3.2.5",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "start": "tsx -r dotenv/config --watch src/server.ts",
